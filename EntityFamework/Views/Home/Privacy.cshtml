@model Student
@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

<p>Use this page to detail your site's privacy policy.</p>
<ul>
    @foreach(var properties in Model.GetType().GetProperties())
    {
        <li>
            @properties.Name: @properties.GetValue(Model)
        </li>
    }
</ul>